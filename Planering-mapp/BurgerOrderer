# Lista över alla varor
hamburgers = {"Cheeseburger": 20, "Naturell": 10}

sides = {"Liten Pommes": 10, "Mellan Pommes": 15, "Stor Pommes": 20}

drinks = ["Coca-Cola", "Fanta", "Sprite", "Vatten"]
sizes = ["Liten", "Mellan", "Stor"]


# Listar priser per storlek
size_price = {"Liten": 10,
              "Mellan": 15,
              "Stor": 20}

#Printar varje drink med sitt pris beroende på storlek
for drink in drinks:
    for size in sizes:
        print(f"{size} {drink}: {size_price[size]} kr")

# Skriver ut hamburgarna med priser
for burger, price in hamburgers.items():
    print(f"{burger}: {price} kr")